<!doctype html>
<html lang="en">
<head>
    <%- include("head.ejs"); %>
</head>
<body>
<section class="col-md-12">
    <div class="container">
        <table class="table">
            <thead>
            <th>Tên</th>
            <th>Địa chỉ</th>
            <th>Điện thoại</th>
            </thead>
            <tbody>
            <% for(var i=0;i<dskh.length;i++){ %>
                <tr>
                    <td><%= dskh[i].Ten %></td>
                    <td><%= dskh[i].DiaChi %></td>
                    <td><%= dskh[i].DienThoai %></td>
                </tr>
            <% } %>
            </tbody>
        </table>
        <table class="table">
            <thead>
            <th>Mã số</th>
            <th>Ngày đặt</th>
            <th>Tổng tiền</th>
            <th>Điện thoại</th>
            </thead>
            <tbody>
            <% dsdh.forEach(function (e) { %>
                <tr>
                    <td><%= e.MaSo %></td>
                    <td><%= e.NgayDat %></td>
                    <td><%= e.TongTien %></td>
                    <td><%= e.DienThoai %></td>
                </tr>
            <% }) %>
            </tbody>
        </table>
        <table class="table">
            <thead>
            <th>Tên SP</th>
            <th>Mô tả</th>
            <th>Đơn vị</th>
            <th>Giá</th>
            </thead>
            <tbody>

            </tbody>
        </table>
    </div>
</section>
</body>
</html>